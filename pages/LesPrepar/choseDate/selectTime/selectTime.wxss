.bottom_bar {
  width: 100%;
  border-top: 1px;
  position: fixed;
  bottom: 0px;
  box-shadow: 0px 1px 4px 1px rgba(195, 194, 194, 0.5);
  padding-top: 17px;
  padding-bottom: 17px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom_bar .next_time {
  width: 656rpx;
  height: 96rpx;
  background: rgba(50, 133, 139, 1);
  border-radius: 8rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 96rpx;
  text-align: center;
}

.time_contenter{
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;

}

.tiem_item {
  width: 308rpx;
  height: 80rpx;
  border-radius: 8rpx;
  border: 2rpx solid rgba(226, 226, 226, 1);
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(22, 8, 10, 1);
  line-height: 80rpx;
  text-align: center;
  margin: 11rpx auto;
}

.tiem_item_no{
  width: 308rpx;
  height: 80rpx;
  border-radius: 8rpx;
  background:rgba(232,231,231,1);
  font-size: 26rpx;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 80rpx;
  text-align: center;
  margin: 11rpx auto;
}

/* 遮罩层 */

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

/* 弹出层 */

.modalDlg {
  width: 468rpx;
  height: 132rpx;
  position: fixed;
  top: 30vh;
  left: 0;
  right: 0;
  z-index: 9999;
  margin: 0 auto;
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(22, 8, 10, 1);
  line-height: 132rpx;
  background: rgba(255, 255, 255, 1);
}
